<%= render "shared/search", url: admin_plans_path, search_parameter: "name" %>
<div class="List">
  <div class="row">
    <h3 class="List-title">Listing Plans</h3>
    <div class="column">
      <table class="table">
        <thead>
          <tr>
            <th>Name</th>
            <th>Price</th>
            <th>Discount</th>
            <th>Start time</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <%= render @plans %>
        </tbody>
      </table>
      <p>
        <%= link_to 'New Plan', new_admin_plan_path %>
      </p>
    </div>
  </div>
</div>
<%= will_paginate @plans, renderer: BootstrapPagination::Rails %>
