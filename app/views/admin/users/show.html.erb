<section class="ShowUser">
	<div class="panel panel-info">
		<div class="panel-heading">
		  <h4 class="panel-title"> <%= @user.email if @user.email %> </h4>
		</div>
		<div class="panel-footer">
			<%= link_to 'Back', admin_users_path, class: 'btn btn-default glyphicon glyphicon-chevron-left', title: 'Back' %>
			<%= link_to '', edit_admin_user_path(@user), class: 'btn btn-primary glyphicon glyphicon-pencil', title: 'Edit' %>
		</div>
	</div>
</section>	
